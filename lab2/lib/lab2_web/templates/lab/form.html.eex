<%= form_for @changeset, @action,[multipart: true], fn f -> %>
        <div class="form-group">
          <label>File</label>
          <%= label f, :file, class="form-control"%>
          <%= file_input f, :file %>
        </div>
        <div class="form-group">
          <%= submit "Submit", class: "btn btn-primary" %>
        </div>
<% end %>